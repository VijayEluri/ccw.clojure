Manifest-Version: 1.0
Bundle-ManifestVersion: 2
Bundle-Name: Clojure Plug-in
Bundle-SymbolicName: clojure;singleton:=true
Bundle-Version: 1.0.0
Bundle-ClassPath: .,
 clojure.jar,
 clojure-sources.jar
Export-Package: clojure,
 clojure.asm,
 clojure.asm.commons,
 clojure.lang,
 clojure.proxy.clojure.lang,
 clojure.proxy.java.lang,
 clojure.proxy.javax.swing.table,
 clojure.util
Bundle-ActivationPolicy: lazy
Bundle-Activator: clojure.util.ClojurePlugin
Require-Bundle: org.eclipse.core.runtime;bundle-version="3.4.0"
Bundle-RequiredExecutionEnvironment: J2SE-1.5
Eclipse-BuddyPolicy: registered
